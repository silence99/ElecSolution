<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="Framework.SeatButton"
	x:Name="UserControl"
	Width="75" d:DesignHeight="480">
 <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="styles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
	<Grid x:Name="LayoutRoot">
		<Button HorizontalAlignment="Left" Height="50" Margin="0,8,0,0" VerticalAlignment="Top" Width="60" Style="{DynamicResource SeatButtonStyle}" Name="button1" Click="button1_Click" FontSize="24" />
		<Image HorizontalAlignment="Left" Height="25" Margin="42,3,0,0" VerticalAlignment="Top" Width="25" Name="_image" />
	</Grid>
</UserControl>