<Page x:Class="Emergence_WPF.SubEventPopup_Map"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Emergence_WPF"
      mc:Ignorable="d" 
      d:DesignHeight="1080" d:DesignWidth="1920"
      Title="SubEventPopup_Map">
    <Page.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../../style/MasterEventDetailStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Page.Resources>
    <Grid Name="Grid_SubEventPopup_Map" Background="White">
        <Grid.RowDefinitions>
            <RowDefinition Height="80px"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <DockPanel Grid.Row="0">
            <Label Style="{StaticResource RightLabel}" FontSize="20px" Margin="19 0 0 0" Content="指挥中心地图标会"/>
        </DockPanel>
        <Border Grid.Row="1" BorderBrush="#006F69" BorderThickness="0 1 0 0" Background="#F9F9F9">
            <Grid Margin="22"></Grid>
        </Border>
    </Grid>
</Page>
